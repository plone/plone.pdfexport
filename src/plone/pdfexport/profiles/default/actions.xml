<?xml version="1.0" encoding="utf-8"?>
<object name="portal_actions" meta_type="Plone Actions Tool"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <object name="document_actions" meta_type="CMF Action Category">
  <property name="title">Document actions</property>
  <object name="aspdf" meta_type="CMF Action" i18n:domain="plone.pdfexport">
   <property name="title" i18n:translate="">PDF</property>
   <property name="description"
      i18n:translate="">Download this Page as PDF</property>
   <property name="url_expr">string:${object_url}/aspdf</property>
   <property name="link_target"></property>
   <property name="icon_expr">string:${globals_view/navigationRootUrl}/++resource++mimetype.icons/pdf.png</property>
   <property name="permissions">
    <element value="View"/>
   </property>
   <property name="visible">True</property>
  </object>
 </object>
</object>
